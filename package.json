{
  "name": "trakta.co",
  "version": "0.27.0",
  "description": "Make a sound!",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "analyze:client": "babel-node internal/scripts/analyze --client",
    "analyze:server": "babel-node internal/scripts/analyze --server",
    "build": "babel-node internal/scripts/build --optimize",
    "build:dev": "babel-node internal/scripts/build",
    "clean": "cross-env babel-node internal/scripts/clean",
    "deploy": "time eb deploy --timeout 30 -l `git describe`",
    "develop": "cross-env DEPLOYMENT=development babel-node internal/development",
    "lint": "standard --fix",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:add": "sequelize migration:create",
    "generate": "all-contributors generate",

    "image:build": "sudo docker build -f ../deployment/trakta.co/Dockerfile -t deanslamajr/traktaco:\"$(git describe)\" .",
    "publish": "npm run image:build",
    "publish:test": "npm version prerelease --preid=g\"$(git rev-parse --short=5 HEAD)\" && npm run publish"
  },
  "standard": {
    "globals": [
      "sinon"
    ],
    "ignore": [
      "/internal/scripts/preinstall.js"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "collectCoverageFrom": [
      "shared/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|flow-typed|public)/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deanslamajr/trakta.co.git"
  },
  "keywords": [
    "react",
    "universal",
    "express",
    "webpack",
    "tone.js",
    "web audio"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deanslamajr/trakta.co/issues"
  },
  "homepage": "https://github.com/deanslamajr/trakta.co#readme",
  "dependencies": {
    "app-root-dir": "1.0.2",
    "autoprefixer": "^7.1.4",
    "aws-sdk": "^2.62.0",
    "axios": "^0.16.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "create-react-class": "^15.6.2",
    "cross-env": "4.0.0",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "express-enforces-ssl": "^1.1.0",
    "format-duration": "^1.0.0",
    "helmet": "3.5.0",
    "hpp": "0.2.2",
    "is-ios": "^1.0.0",
    "lamejs": "^1.2.0",
    "lodash.isequal": "^4.5.0",
    "modernizr": "3.5.0",
    "moment": "^2.21.0",
    "nocache": "^2.0.0",
    "normalize.css": "6.0.0",
    "offline-plugin": "4.7.0",
    "pg": "^6.4.0",
    "postcss": "^6.0.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-simple-vars": "^4.0.0",
    "prop-types": "15.5.8",
    "rand-token": "^0.4.0",
    "random-words": "0.0.1",
    "react": "^16.4.0",
    "react-async-bootstrapper": "1.1.1",
    "react-async-component": "1.0.0-beta.3",
    "react-dom": "^16.4.0",
    "react-helmet": "5.0.3",
    "react-icons": "^2.2.7",
    "react-key-handler": "^1.2.0-beta.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "4.1.1",
    "react-slider": "^0.11.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "s3-upload-stream": "^1.0.7",
    "sequelize": "^4.3.0",
    "serialize-javascript": "1.3.0",
    "tone": "^0.10.0",
    "uuid": "^3.0.1",
    "viewport-dimensions": "^0.2.0",
